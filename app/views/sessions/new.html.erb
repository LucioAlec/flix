<h1> Sign in <h1>
<h6>
  No account yet? <%= link_to "Sign up!", signin_path %>
</h6>
<%= form_with(url: session_path) do |f| %> 
  <%= f.label :email_or_username %>
  <%= f.text_field :email_or_username, autofocus: true, require: true %>

  <%= f.label :password %>
  <%= f.password_field :password, require: true %>

  <%= f.submit "Sign In" %>
<%end%> 